{
  "name": "blast-off",
  "version": "2.5.0",
  "description": "A basic template and project for static sites",
  "source": "src/index.html",
  "parcel-namer-rewrite": {
    "hashing": "never",
    "disable": false
  },
  "@parcel/transformer-css": {
    "drafts": {
      "nesting": true
    }
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build --public-url ./",
    "copy-assets": "cp -vR ./src/assets ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matoululu/blast-off.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matoululu/blast-off/issues"
  },
  "homepage": "https://github.com/matoululu/blast-off#readme",
  "devDependencies": {
    "buffer": "^5.7.1",
    "parcel": "latest",
    "parcel-namer-rewrite": "^2.0.0-rc.3"
  }
}
